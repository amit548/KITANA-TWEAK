logs=\$stored'/LOGS/KI00Governor.log' 2>/dev/null
backup=$stored'/BACKUP' 2>/dev/null

if [ -f \$logs ]
then
	$busybox rm \$logs 2>/dev/null
fi

\$busybox echo 'Applying Change...' | \$busybox tee -a \$logs
govd="\$backup/KI00CPU_Governor" 2>/dev/null
governor="/sys/bus/cpu/devices/cpu0/cpufreq/scaling_governor" 2>/dev/null
\$busybox chmod 755 \$governor 2>/dev/null
\$busybox echo "\$(\$busybox cat \$govd)" > \$governor
\$busybox chmod 644 \$governor 2>/dev/null
CPUPATH="/sys/bus/cpu/devices/cpu*" 2>/dev/null
for CPUON in \$CPUPATH
do
	\$busybox chmod 755 \$CPUON/online
	\$busybox echo '1' > \$CPUON/online
	\$busybox chmod 644 \$CPUON/online
done
\$busybox echo 'OK done !!!' | \$busybox tee -a \$logs
KITANA
