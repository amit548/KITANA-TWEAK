	else
		$busybox sed -i '/ACTIVITY_INACTIVE_RESET_TIME=false/d' $build
		$busybox sed -i '/CPU_MIN_CHECK_DURATION=false/d' $build
		$busybox sed -i '/ENFORCE_PROCESS_LIMIT=false/d' $build
