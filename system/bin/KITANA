media_server_kill ( )
{
	if [ "$(pgrep media)" ] && [ "$(pgrep mediaserver)" ]
	then
		$busybox killall -9 android.process.media 2 2>/dev/null
		$busybox killall -9 mediaserver 2 2>/dev/null
	fi
}
