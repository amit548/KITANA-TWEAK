$busybox chown 0:0 $stored'/KI00ExtScan' 2>/dev/null
#
#
# Wlan Ws Wakelock Enabler
#
#
$busybox cat >> $stored'/KI00Enable_Wlan_Ws' << KITANA
#!/system/bin/sh

magisk='/sbin/.core/img' 2>/dev/null
busybox='/sbin/.core/mirror/bin/busybox' 2>/dev/null
core=\$magisk'/KitanaTweak' 2>/dev/null
system=\$core'/system' 2>/dev/null
stored=\$system'/KITANA' 2>/dev/null
logs=\$stored'/LOGS/KI00Enable_Wlan_Ws.log' 2>/dev/null

if [ -f \$logs ]
then
	$busybox rm \$logs 2>/dev/null
fi

\$busybox echo 'Enabling Wlan Ws Wakelock...' | \$busybox tee -a \$logs
if [ -e /sys/module/wakeup/parameters/enable_msm_hsic_ws ]
then
	\$busybox echo 'Y' > /sys/module/wakeup/parameters/enable_wlan_wake_ws
	\$busybox echo 'OK Done !!!' | \$busybox tee -a \$logs
else
	\$busybox echo 'Not Supported !!!' | \$busybox tee -a \$logs
fi
KITANA
$busybox chmod 755 $stored'/KI00Enable_Wlan_Ws' 2>/dev/null
$busybox chown 0:0 $stored'/KI00Enable_Wlan_Ws' 2>/dev/null
#
#
########################################################################################################################################################################
#
