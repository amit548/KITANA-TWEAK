#!/system/bin/sh

magisk='/sbin/.core/img' 2>/dev/null
busybox='/sbin/.core/mirror/bin/busybox' 2>/dev/null
core=\$magisk'/KitanaTweak' 2>/dev/null
system=\$core'/system' 2>/dev/null
stored=\$system'/KITANA' 2>/dev/null
logs=\$stored'/LOGS/KI00Fstrim.log' 2>/dev/null

if [ -f \$logs ]
then
	$busybox rm \$logs 2>/dev/null
fi

\$busybox echo 'Applying Fstrim...' | \$busybox tee -a \$logs
if [ -d /cache ]
then
	\$busybox fstrim -v /cache | \$busybox tee -a \$logs
fi
if [ -d /data ]
then
	\$busybox fstrim -v /data | \$busybox tee -a \$logs
fi
if [ -d /system ]
then
	\$busybox fstrim -v /system | \$busybox tee -a \$logs
fi
if [ -d /magisk ]
then
	\$busybox fstrim /magisk
fi
if [ -d /preload ]
then
	\$busybox fstrim -v /preload | \$busybox tee -a \$logs
fi
if [ -d /data/sdext2 ]
then
	\$busybox fstrim -v /data/sdext2 | \$busybox tee -a \$logs
fi
\$busybox echo 'Fstrim is done !!!' | \$busybox tee -a \$logs
KITANA
$busybox chmod 755 $stored'/KI00Fstrim' 2>/dev/null
$busybox chown 0:2000 $stored'/KI00Fstrim' 2>/dev/null
