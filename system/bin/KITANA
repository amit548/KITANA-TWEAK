fstrim_now ( )
{
	$busybox echo 'Applying Fstrim...'
	$busybox sleep 1
	$busybox echo ''
	if [ -d /cache ]
	then
		$busybox fstrim -v /cache
		$busybox sleep 0.5
	fi
	if [ -d /data ]
	then
		$busybox fstrim -v /data
		$busybox sleep 0.5
	fi
	if [ -d /system ]
	then
		$busybox fstrim -v /system
		$busybox sleep 0.5
	fi
	if [ -d /magisk ]
	then
		$busybox fstrim -v /magisk
		$busybox sleep 0.5
	fi
	if [ -d /preload ]
	then
		$busybox fstrim -v /preload
		$busybox sleep 0.5
	fi
	if [ -d /data/sdext2 ]
	then
		$busybox fstrim -v /data/sdext2
		$busybox sleep 0.5
	fi
	$busybox echo ''
	$busybox sleep 1
	$busybox echo 'Fstrim is done !!!'
}
