#!/system/bin/sh

magisk='/sbin/.core/img' 2>/dev/null
busybox='/sbin/.core/mirror/bin/busybox' 2>/dev/null
core=\$magisk'/KitanaTweak' 2>/dev/null
system=\$core'/system' 2>/dev/null
stored=\$system'/KITANA' 2>/dev/null
logs=\$stored'/LOGS/KI00Ram_Manager.log' 2>/dev/null

if [ -f \$logs ]
then
	$busybox rm \$logs 2>/dev/null
fi

\$busybox echo 'Ram Manager Balance is applying...' | \$busybox tee -a \$logs
if [ -e /sys/module/lowmemorykiller/parameters/cost ]
then
	\$busybox chmod 644 /sys/module/lowmemorykiller/parameters/cost 2>/dev/null
	\$busybox echo '32' > /sys/module/lowmemorykiller/parameters/cost
fi
if [ -e /sys/module/lowmemorykiller/parameters/debug_level ]
then
	\$busybox chmod 644 /sys/module/lowmemorykiller/parameters/debug_level 2>/dev/null
	\$busybox echo '0' > /sys/module/lowmemorykiller/parameters/debug_level
fi
RAM=\$((\$(\$busybox awk '/MemTotal/{print \$2}' /proc/meminfo)/1024)) 2>/dev/null
lmk1=\$((RAM*3*1024/100/4));
lmk2=\$((RAM*4*1024/100/4));
lmk3=\$((RAM*5*1024/100/4));
lmk4=\$((RAM*7*1024/100/4));
lmk5=\$((RAM*9*1024/100/4));
lmk6=\$((RAM*10*1024/100/4));
if [ -e /sys/module/lowmemorykiller/parameters/minfree ]
then
	\$busybox chmod 644 /sys/module/lowmemorykiller/parameters/minfree 2>/dev/null
	\$busybox echo "\$lmk1,\$lmk2,\$lmk3,\$lmk4,\$lmk5,\$lmk6" > /sys/module/lowmemorykiller/parameters/minfree
	\$busybox echo "Balanced RAM manager for \$RAM mb devices successfully applied..." | \$busybox tee -a \$logs
else
	\$busybox echo "Balanced RAM manager for \$RAM mb devices successfully not applied !!!" | \$busybox tee -a \$logs
fi
KITANA
$busybox chmod 755 $stored'/KI00Ram_Manager_Balance' 2>/dev/null
$busybox chown 0:0 $stored'/KI00Ram_Manager_Balance' 2>/dev/null
