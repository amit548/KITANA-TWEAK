		$busybox echo 'debug.sf.hw=1' >> $build
		$busybox echo 'debug.performance.tuning=1' >> $build
		$busybox echo 'video.accelerate.hw=1' >> $build
		$busybox echo 'debug.egl.profiler=1' >> $build
		$busybox echo 'debug.egl.hw=1' >> $build
