	pm disable com.google.android.gms/com.google.android.location.internal.server.GoogleLocationService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.geocode.GeocodeService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.network.NetworkLocationService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.internal.GoogleLocationManagerService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.geofencer.service.GeofenceProviderService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.fused.service.FusedProviderService 2>/dev/null
	pm disable com.google.android.gms/com.google.android.location.reporting.service.DispatchingService 2>/dev/null
	pm disable com.google.android.gms/com.google.location.nearby.direct.service.NearbyDirectService 2>/dev/null
}
